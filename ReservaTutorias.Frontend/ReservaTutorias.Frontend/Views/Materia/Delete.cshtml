@model ReservaTutorias.Frontend.Models.Materia

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row justify-content-center">
    <div class="col-md-10">
        <h3>Debe confirmar la acción para eliminar la materia</h3>
        <div>
            <h4>Materia</h4>
            <hr />
            <dl class="row">
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.IdMateria)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.IdMateria)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.NombreMateria)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.NombreMateria)
                </dd>
            </dl>

            <form asp-action="Delete">
                <input type="submit" value="Eliminar" class="btn btn-danger" /> |
                <a asp-action="Index">Volver</a>
            </form>
        </div>
    </div>
</div>
        
