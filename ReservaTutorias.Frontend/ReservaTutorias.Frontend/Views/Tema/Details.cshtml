@model ReservaTutorias.Frontend.Models.Tema

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row justify-content-center">
    <div class="col-md-10">
        <div>
            <h4>Detalle de tema</h4>
            <hr />
            <dl class="row">
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.NombreTema)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.NombreTema)
                </dd>
                <dt class="col-sm-2">
                    Materia
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Materia.NombreMateria)
                </dd>
            </dl>
        </div>
        <div>
            @Html.ActionLink("Editar", "Edit", new { id = Model.IdTema }) |
            <a asp-action="Index">Volver</a>
        </div>
    </div>
</div>

    