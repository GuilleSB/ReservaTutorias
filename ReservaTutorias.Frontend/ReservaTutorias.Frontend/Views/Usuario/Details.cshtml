@model ReservaTutorias.Frontend.Models.Usuario

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row justify-content-center">
    <div class="col-md-10">
        <div>
            <h4>Detalles de usuario</h4>
            <hr />
            <dl class="row">
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Cedula)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Cedula)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Nombre)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Nombre)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Apellidos)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Apellidos)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Correo)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Correo)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.TipoUsuario)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.TipoUsuario)
                </dd>
            </dl>
        </div>
        <div>
            @Html.ActionLink("Editar", "Edit", new { id = Model.IdUsuario }) |
            <a asp-action="Index">Volver</a>
        </div>
    </div>
</div>


